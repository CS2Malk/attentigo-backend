{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "FirstName": {
      "type": "string",
      "required": true
    },
    "LastName": {
      "type": "string",
      "required": true
    },
    "Email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "Password": {
      "type": "password",
      "required": true,
      "minLength": 8
    },
    "school": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::school.school",
      "inversedBy": "students"
    },
    "attendances": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::attendance.attendance",
      "mappedBy": "student"
    }
  }
}
