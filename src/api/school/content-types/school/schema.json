{
  "kind": "collectionType",
  "collectionName": "schools",
  "info": {
    "singularName": "school",
    "pluralName": "schools",
    "displayName": "School"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Latitude": {
      "type": "string",
      "required": true
    },
    "Longitude": {
      "type": "string",
      "required": true
    },
    "students": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::student.student",
      "mappedBy": "school"
    },
    "startTime": {
      "type": "time",
      "required": true
    }
  }
}
